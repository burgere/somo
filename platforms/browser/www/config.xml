<?xml version='1.0' encoding='utf-8'?>
<widget id="com.github.somo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SOMO</name>
    <description>
        Cordova-AngularJS-angularMaterial Seed.
    </description>
    <author email="marioaleo@gmail.com" href="https://github.com/mario-aleo">
        MarioAleo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
